
@{
    ViewData["Title"] = "Search Object";
}

<form method="post">
    <div class="form-group">
        <div class="col-md-10">
            <label for="objectid">Object Id</label>
            <input type="number" name="objectid" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <label for="objectType">Object Type</label>
            <input type="text" name="objectType" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <label for="timestamp">Timestamp</label>
            <input type="number" name="timestamp" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input id="search" type="button" value="Search" />
        </div>
    </div>
</form>

<div>
    <h2>Object State</h2>
    <div id="searchResults">

    </div>
</div>

<div id="error_section" style="display: none;">
    <h3>Processing Error</h3>
    <p>Error code ---> <span id="error_code"></span></p>
    <p>Error Message ---> <span id="error_msg"></span></p>
    <p>Response Message ---> <span id="res_msg"></span></p>
</div>
