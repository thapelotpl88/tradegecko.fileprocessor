
@{
	ViewData["Title"] = "File Upload";
}

<h1>File Upload</h1>
<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="FileUploadSave">
	<div class="form-group">
		<div class="col-md-10">
			<p>Upload file using this form:</p>
			<input type="file" name="files" />
		</div>
	</div>
	<div class="form-group">
		<div class="col-md-10">
			<input type="submit" value="Upload" />
		</div>
	</div>
</form>
